author = {Jose Sanchez and Juan-Antonio Corrales and Belhassen-Chedli Bouzgarrou and Youcef Mezouar},
title ={Robotic manipulation and sensing of deformable objects in domestic and industrial applications: a survey},
journal = IJRR,
volume = {37},
