@INPROCEEDINGS{Bayazit2005,
  author = {Bayazit, O. B. and Xie, Dawen and Amato, N. M.},
  title = {Iterative relaxation of constraints: a framework for improving automated
	motion planning},
  booktitle = IROS,
  year = {2005},
  pages = {3433--3440},
  abstract = {This paper presents a technique for improving the efficiency of automated
	motion planners. Motion planning has application in many areas such
	as robotics, virtual reality systems, computer-aided design, and
	even computational biology. Although there have been steady advances
	in motion planning algorithms, especially in randomized approaches
	such as probabilistic roadmap methods (PRMs) or rapidly-exploring
	random trees (RRTs), there are still some classes of problems that
	cannot be solved efficiently using these state-of-the-art motion
	planners. In this paper, we suggest an iterative strategy addressing
	this problem where we first simplify the problem by relaxing some
	feasibility constraints, solve the easier version of the problem,
	and then use that solution to help us find a solution for the harder
	problem. We show how this strategy can be applied to rigid bodies
	and to linkages with high degrees of freedom, including both open
	and closed chain systems. Experimental results are presented for
	linkages composed of 9-98 links. Although we use PRMs as the automated
	planner, the framework is general and can be applied with other motion
	planning techniques as well.},
  file = {Bayazit2005.pdf:Bayazit2005.pdf:PDF},
  keywords = {path planning, probability, automated motion planning, iterative constraint
	relaxation, probabilistic roadmap},
  owner = {Dmitry},
  review = {This paper proposes to solve constrained planning by first solving
	a relaxed version of the problem and then using that solution as
	a starting point for more and more constrained versions of the problem.
	It is unclear whether the solution to a less constrained problem
	will provide any speed-up to a more constrained problem. For instance,
	if there is a narrow passage in the less-constrained problem and
	the planner plans a path through this region, the passage may close
	in a more constrained version of the problem and the previous solution
	will be useless or potentially harmful because the planner starts
	with a bad initial guess that it has to escape from.},
  timestamp = {2009.03.14}
}

